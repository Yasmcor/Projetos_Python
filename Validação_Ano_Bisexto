# Importando pacote date.
from datetime import date

ano = int(input('Digite o ano'))
resultado = ano % 4
if ano == 0:
    ano = date.today().year
if resultado == 0 and ano % 100 != 0 or ano % 400 == 0:
    print('O ano {} é Bissexto'.format(ano))
else:
    print('O ano {} não é Bissexto'.format(ano))
